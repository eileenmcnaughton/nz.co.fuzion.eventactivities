<?xml version="1.0"?>
<extension key="nz.co.fuzion.eventactivities" type="module">
  <file>eventactivities</file>
  <name>Event Activities</name>
  <description>Create activities reflecting event attendance</description>
  <license>AGPL</license>
  <maintainer>
    <author>Eileen</author>
    <email>eileen@fuzion.co.nz</email>
  </maintainer>
  <releaseDate>2014-02-27</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.2</ver>
    <ver>4.4</ver>
  </compatibility>
  <comments>This extension creates activities to reflect event attendance with the date and time corresponding to the event. This allows event activity to be visible in the activity 'timeline'</comments>
  <civix>
    <namespace>CRM/Eventactivities</namespace>
  </civix>
</extension>
